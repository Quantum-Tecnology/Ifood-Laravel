{
  "name": "quantumtecnology/ifood-laravel",
  "type": "library",
  "license": "MIT",
  "homepage": "http://www.gustavosantarosa.esy.es/",
  "description": "ifood for Laravel",
  "keywords": [
    "quantumtecnology",
    "ifood-laravel",
    "laravel",
    "ifood",
    "php"
  ],
  "require": {
    "php": "^8.0",
    "laravel/framework": "^12.0",
    "illuminate/support": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.64"
  },
  "autoload": {
    "psr-4": {
      "QuantumTecnology\\IfoodLaravel\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "QuantumTecnology\\IfoodLaravel\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Luis Gustavo Santarosa",
      "email": "gustavo-computacao@hotmail.com",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "gustavo-computacao@hotmail.com"
  },
  "minimum-stability": "stable",
  "extra": {
    "laravel": {
      "providers": [
        "QuantumTecnology\\IfoodLaravel\\Provider\\IfoodLaravelProvider"
      ]
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
